  "engines": {
    "node": "^14.7.0"
  },
  "lint-staged": {
    "./**/*.{json,yaml,md}": "prettier --write",
    "./**/*.{js,jsx}": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "./.githooks/post-merge"
    }
